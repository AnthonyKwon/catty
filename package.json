{
  "name": "chattybot",
  "version": "2.0.0-alpha.0",
  "description": "Discord Text-To-Speech Bot",
  "main": "./out/main.js",
  "scripts": {
    "build": "tsc",
    "postbuild": "copyfiles -u 1 ./src/**/*.json ./bin/",
    "start": "node .",
    "debug": "NODE_ENV=maintenance nodemon --exec 'node --inspect --require ts-node/register ./src/main.ts'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AnthonyKwon/chattybot.git"
  },
  "author": "Anthony Kwon",
  "license": "GPL-2.0-or-later",
  "_moduleAliases": {
    "@commands": "./bin/commands",
    "@modules": "./bin/modules"
  },
  "dependencies": {
    "@discordjs/opus": "github:discordjs/opus",
    "@discordjs/voice": "^0.8.0",
    "@google-cloud/text-to-speech": "^3.2.0",
    "discord.js": "^14.7.1",
    "module-alias": "^2.2.2",
    "nodemon": "^2.0.7",
    "sodium": "^3.0.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "cpy-cli": "^4.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
